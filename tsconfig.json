{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "isolatedModules": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "noImplicitAny": true,
        "outDir": "./bin",
        "suppressImplicitAnyIndexErrors": true,
        "jsx": "react"
    },
    "exclude": ["node_modules"]
}